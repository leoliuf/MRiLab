         <rfSinc AnchorTE="$1'on','off'" Apod="$2'Non','Hamming','Hanning'" CoilID="1" DupSpacing="0" Duplicates="1" FA="VCtl.FlipAng" Notes="sinc rf pulse" Switch="$1'on','off'" TBP="4" dt="20e-6" rfFreq="0" rfPhase="0" tEnd="CV4+0.5e-3" tStart="CV4"/>
         <rfSinc AnchorTE="$2'on','off'" Apod="$2'Non','Hamming','Hanning'" CoilID="1" DupSpacing="0" Duplicates="1" FA="0" Notes="sinc rf pulse" Switch="$1'on','off'" TBP="4" dt="0.05e-3" rfFreq="0" rfPhase="0" tEnd="VCtl.TR" tStart="CV4+0.6e-3"/>
      </rf>
      <GzSS>
         <GzCartesian DupSpacing="0" Duplicates="1" Gz1Sign="1" Gz2Sign="0" Notes="cartesian phase" Switch="$1'on','off'" t1End="CV4+CV2" t1Start="CV4+CV1" t2End="VCtl.TE+CV2" t2Start="VCtl.TE+CV1" tRamp="100e-6"/>
      </GzSS>
      <GyPE>
         <GyCartesian DupSpacing="0" Duplicates="1" Gy1Sign="1" Gy2Sign="0" Notes="cartesian phase" Switch="$1'on','off'" t1End="CV4+CV2" t1Start="CV4+CV1" t2End="VCtl.TE+CV2" t2Start="VCtl.TE+CV1" tRamp="100e-6"/>
