<?xml version="1.0" encoding="utf-8"?>
<MRiLabSeq Name="PSD_SPGR3DME" Notes="3D spoiled gradient echo sequence">
   <CVs CV1="2e-3" CV10="0" CV11="0" CV12="0" CV13="0" CV14="0" CV2="3e-3" CV3="abs(rem(VVar.TRCount,2)*2-1)" CV4="0e-3" CV5="0" CV6="0" CV7="0" CV8="0" CV9="0"/>
   <Specials DummyPulse="^1" ME="^1"/>
   <Pulses Freq="1" Notes="regular TR section" Switch="$1'on','off'" TREnd="Inf" TRStart="1" tE="VCtl.TR" tS="0">
